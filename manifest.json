{
  "manifest_version": 2,

  "name": "Titan Manager",
  "description": "What you need",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "identity",
    "activeTab",
    "fileBrowserHandler",
    "system.memory",
    "system.storage",
    "https://ajax.googleapis.com/"
  ]
}